    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) Launch",
            "preLaunchTask": "Build Compiler",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bazel-out/darwin-dbg/bin/src/main",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/bazel-out/darwin-dbg/bin/src/main.runfiles/__main__/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "osx": {
                "type": "cppdbg",
                "MIMode": "lldb",
                "program": "${workspaceFolder}/bazel-out/darwin-dbg/bin/src/main",
                "cwd": "${workspaceFolder}/bazel-out/darwin-dbg/bin/src/main.runfiles/__main__/",
            },
            "linux": {
                "type": "cppdbg",
                "MIMode": "gdb",
                "program": "${workspaceFolder}/bazel-out/local_linux-dbg/bin/src/main",
                "cwd": "${workspaceFolder}/bazel-out/local_linux-dbg/bin/src/main.runfiles/__main__/",
            },
        }
    ]
}